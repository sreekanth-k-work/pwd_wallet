<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/wallet_app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/wallet_app_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.PasswordWallet"
        tools:targetApi="31">
        <activity
            android:name="com.example.passwordmanager.MainScreenActivity"
            android:exported="true"
            android:theme="@style/Theme.PasswordWallet">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name="com.example.passwordmanager.AddDetailsScreenActivity"/>
        <activity android:name="com.passwordmanager.passwordwallet.DetailActivity"/>
    </application>
</manifest>